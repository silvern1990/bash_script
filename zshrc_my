source .zshrc_api_key
alias vim='nvim'
alias todo='todo.sh'
alias ssh-nas='openssl enc -aes-256-cbc -d -in ~/.ssh/exp/ssh-nas.enc -out /tmp/exp && expect /tmp/exp'
. "$HOME/.local/bin/env"

